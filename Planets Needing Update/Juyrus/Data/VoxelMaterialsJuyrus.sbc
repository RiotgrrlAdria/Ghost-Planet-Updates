      <ExtXZnYFar2>Textures\Voxels\Dummy_add.dds</ExtXZnYFar2>

      <!--Foliage-->
      <FoliageColorTextureArray>
        <Color>Textures\Models\Environment\Grass\AlienGrass_01_cm.dds</Color>
        <Color>Textures\Models\Environment\Grass\AlienGrass_02_cm.dds</Color>
        <Color>Textures\Models\Environment\Grass\AlienGrass_03_cm.dds</Color>
        <Color>Textures\Models\Environment\Grass\AlienGrass_04_cm.dds</Color>
        <Color>Textures\Models\Environment\Grass\AlienGrass_05_cm.dds</Color>
        <Color>Textures\Models\Environment\Grass\AlienGrass_06_cm.dds</Color>
        <Color>Textures\Models\Environment\Grass\AlienGrass2_08_cm.dds</Color>
      </FoliageColorTextureArray>
      
      <FoliageNormalTextureArray>
        <Normal>Textures\Models\Environment\Grass\AlienGrass_01_ng.dds</Normal>
        <Normal>Textures\Models\Environment\Grass\AlienGrass_02_ng.dds</Normal>
        <Normal>Textures\Models\Environment\Grass\AlienGrass_03_ng.dds</Normal>
        <Normal>Textures\Models\Environment\Grass\AlienGrass_04_ng.dds</Normal>
        <Normal>Textures\Models\Environment\Grass\AlienGrass_05_ng.dds</Normal>
        <Normal>Textures\Models\Environment\Grass\AlienGrass_06_ng.dds</Normal>
        <Normal>Textures\Models\Environment\Grass\AlienGrass2_08_ng.dds</Normal>
      </FoliageNormalTextureArray>

      <FoliageDensity>0.001</FoliageDensity>
      <FoliageScale>
        <X>3.8</X>
        <Y>3.8</Y>
      </FoliageScale>
      <FoliageRandomRescaleMult>0.20</FoliageRandomRescaleMult>

      <SpawnChannels>
        <Channel>1</Channel>
        <Channel>4</Channel>
      </SpawnChannels>
      
      <SpawnsFlora>true</SpawnsFlora>>
      <BiomeValueMin>255</BiomeValueMin>
      <BiomeValueMax>255</BiomeValueMax>  
    </VoxelMaterial>
 
