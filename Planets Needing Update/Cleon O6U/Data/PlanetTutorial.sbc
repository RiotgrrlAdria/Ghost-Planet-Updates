						<Layer Material="Iron_01" Depth="5"/> 			               
					</Layers>
					<Height Min="0" Max="1"/>
					<Latitude Min="0" Max="90"/>
					<Slope Min="20" Max="90"/>
				</Rule>
		  
			</MaterialGroup>
		</ComplexMaterials>

		<EnvironmentItems>
			<Item>
				<Biomes>
					<Biome>0</Biome>
				</Biomes>
				<Materials>
					<Material>Snow</Material>
				</Materials>
				<Items>
					<Item TypeId="MyObjectBuilder_VoxelMapStorageDefinition" GroupId="StoneCoverageIronCore" ModifierId="IcyMountains" Density="0.013" />
				</Items>
				<Rule>
					<Height Min="0" Max="1.2"/>
					<Latitude Min="0" Max="90"/>
					<Slope Min="0" Max="90"/>
				</Rule>
			</Item>
		</EnvironmentItems>

		<MaximumOxygen>0.5</MaximumOxygen>
		<SurfaceGravity>0.90</SurfaceGravity>
		<DefaultSurfaceMaterial Material="Snow" MaxDepth="4"/>
		<DefaultSubSurfaceMaterial Material="IcyMountains"/>
		<HasAtmosphere>true</HasAtmosphere>
	  
		<Atmosphere>
			<Breathable>true</Breathable>
			<OxygenDensity>0.5</OxygenDensity>
			<Density>1.1</Density>
			<LimitAltitude>2.0</LimitAltitude>
			<MaxWindSpeed>200</MaxWindSpeed>
