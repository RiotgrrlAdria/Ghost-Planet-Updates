		<AtmosphereSettings>
			<RayleighScattering>
				<X>31.431</X>
				<Y>42.872</Y>
				<Z>61.941</Z>
			</RayleighScattering>
			<MieScattering>50</MieScattering>
			<MieColorScattering>
				<X>14.938</X>
				<Y>45.661</Y>
				<Z>95.375</Z>
			</MieColorScattering>
			<RayleighHeight>12.098</RayleighHeight>
			<RayleighHeightSpace>8.192</RayleighHeightSpace>
			<RayleighTransitionModifier>0.4</RayleighTransitionModifier>
